baseurl=http://pkgrepos.sktvl.com/skt/centos/$releasever/base/$basearch/
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-CentOS-7-Skt

[skt-core]
